{
	"name": "energy-genius",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm run dev:ui\" \"npm run dev:worker\" --kill-others",
		"dev:ui": "vite build --watch --mode development",
		"dev:worker": "wrangler dev --port 8787",
		"build": "vite build --mode production",
		"deploy": "npm run build && npm run deploy:worker",
		"deploy:worker": "wrangler deploy && npm run verify:deployment",
		"verify:deployment": "node scripts/verify-deployment.js",
		"preview": "vite preview",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"help": "node scripts/help.js",
		"cf-typegen": "wrangler types"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"@cloudflare/workers-types": "^4.20251111.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react": "^5.1.0",
		"autoprefixer": "^10.4.22",
		"concurrently": "^9.2.1",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.5.2",
		"vite": "^7.2.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.46.0"
	},
	"dependencies": {
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-progress": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-tabs": "^1.1.13",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.553.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss-animate": "^1.0.7"
	}
}
